{
  "name": "retro-tank",
  "version": "1.0.0",
  "description": "retro game with javascript",
  "private": true,
  "scripts": {
    "test": "jest",
    "watch": "jest --watchAll",
    "start": "npm run build:dev",
    "build:dev": "webpack-dev-server --env dev --mode none",
    "build:prod": "webpack --env prod --mode production && cp src/ARCADECLASSIC.TTF dist/" 
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bergsans/retro-tank.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bergsans/retro-tank/issues"
  },
  "homepage": "https://github.com/bergsans/retro-tank#readme",
  "devDependencies": {
    "autoprefixer": "^8.6.0",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "@types/jest": "^24.0.23",
    "ts-jest": "^24.2.0",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.2.1",
    "webpack-md5-hash": "0.0.6"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
